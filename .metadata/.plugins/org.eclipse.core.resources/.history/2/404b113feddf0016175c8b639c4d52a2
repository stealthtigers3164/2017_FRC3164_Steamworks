package org.usfirst.frc.team3164.robot.electrical;

import org.usfirst.frc.team3164.robot.motor.MotorController;

public class DriveTrain<T implements MotorController>  {
	//Left Motors
	private MotorController m_frontLeft;
	private MotorController m_backLeft;
	
	//Right Motors
	private MotorController m_frontRight;
	private MotorController m_backRight;
	
	
	
	public MotorController getFrontLeft() {
		return m_frontLeft;
	}
	
	public MotorController getBackLeft() {
		return m_backLeft;
	}
	
	public MotorController getFrontRight() {
		return m_frontRight;
	}
	
	public MotorController getBackRight() {
		return m_backRight;
	}
}
